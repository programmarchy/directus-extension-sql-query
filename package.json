{
	"name": "directus-extension-sql-query",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-operation"
	],
	"type": "module",
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.mjs",
			"api": "dist/api.mjs"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.1.6"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.6",
		"@types/node": "^20.4.2",
		"typescript": "^5.1.6",
		"vue": "^3.3.4"
	}
}
